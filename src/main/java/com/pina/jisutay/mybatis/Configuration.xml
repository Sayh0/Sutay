<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type 의 별칭을 등록 해 놓고  Mapper xml 에서 사용할수 있다. -->
	<typeAliases>
		<typeAlias type="com.pina.jisutay.travel.dto.TravelDto" alias="travelDto"/>
		<typeAlias type="com.pina.jisutay.room_SH.dto.Room_SHDto" alias="room_SHDto"/>
	</typeAliases>
	<!-- 필요한 Mapper xml 등록하기  -->
	<mappers>
		<mapper resource="com/pina/jisutay/mybatis/TravelMapper.xml"/>
		<mapper resource="com/pina/jisutay/mybatis/Room_SHMapper.xml"/>
	</mappers>
</configuration>